<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormChange354"
			editable="true">
			<f:title>
				<core:Title text="Project Data" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="3"
					labelSpanL="3"
					labelSpanM="3"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="4"
					emptySpanL="4"
					emptySpanM="4"
					emptySpanS="0"
					columnsXL="1"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer id="formContainer">
					<f:formElements>
						<f:FormElement label="ProjectUUID">
							<f:fields>
								<Input value="{ProjectUUID}" id="uuid"/>
							</f:fields>
						</f:FormElement>
						
						<f:FormElement label="Projdef">
							<f:fields>
								<Input value="{Projdef}" id="proj" change="onProjdefChange"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Responsibility">
							<f:fields>
								<Input value="{Responsibility}" id="resp" change="onResponsibilityChange"/>
							
							</f:fields>
						</f:FormElement>
						<f:FormElement label="StartDate">
							<f:fields>
							
								<Input value="{StartDate}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="EndDate">
							<f:fields>
								<Input value="{EndDate}" />
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>